{
    "name": "google/cloud-service-management",
    "description": "Google Cloud Service Management Client for PHP",
    "license": "Apache-2.0",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Google\\Cloud\\ServiceManagement\\": "src",
            "GPBMetadata\\Google\\Api\\Servicemanagement\\": "metadata"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Google\\Cloud\\ServiceManagement\\Tests\\": "tests"
        }
    },
    "extra": {
        "component": {
            "id": "cloud-service-management",
            "path": "ServiceManagement",
            "entry": null,
            "target": "googleapis/google-cloud-php-service-management.git"
        }
    },
    "require": {
        "google/gax": "^1.7.0",
        "google/common-protos": "^1.3.1||^2.0||^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0|^8.0",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "suggest": {
        "ext-grpc": "Enables use of gRPC, a universal high-performance RPC framework created by Google.",
        "ext-protobuf": "Provides a significant increase in throughput over the pure PHP protobuf implementation. See https://cloud.google.com/php/grpc for installation instructions."
    }
}
