{
    "name": "google/cloud-kms",
    "description": "Cloud KMS Client for PHP",
    "license": "Apache-2.0",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Google\\Cloud\\Kms\\": "src",
            "GPBMetadata\\Google\\Cloud\\Kms\\": "metadata"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Google\\Cloud\\Kms\\Tests\\": "tests"
        }
    },
    "extra": {
        "component": {
            "id": "cloud-kms",
            "path": "Kms",
            "entry": null,
            "target": "googleapis/google-cloud-php-kms.git"
        }
    },
    "require": {
        "php": ">=7.4",
        "google/gax": "^1.19.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "google/cloud-core": "^1.39"
    },
    "suggest": {
        "ext-grpc": "Enables use of gRPC, a universal high-performance RPC framework created by Google.",
        "ext-protobuf": "Provides a significant increase in throughput over the pure PHP protobuf implementation. See https://cloud.google.com/php/grpc for installation instructions."
    }
}
