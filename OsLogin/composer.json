{
    "name": "google/cloud-oslogin",
    "description": "Google Cloud OsLogin Client for PHP",
    "license": "Apache-2.0",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Google\\Cloud\\OsLogin\\": "src",
            "GPBMetadata\\Google\\Cloud\\Oslogin\\": "metadata"
        }
    },
    "extra": {
        "component": {
            "id": "cloud-oslogin",
            "path": "OsLogin",
            "entry": null,
            "target": "GoogleCloudPlatform/google-cloud-php-oslogin.git"
        }
    },
    "require": {
        "google/gax": "^0.36"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0",
        "google/cloud-core": "^1.14",
        "phpdocumentor/reflection": "^3.0"
    },
    "suggest": {
        "ext-protobuf": "Provides a significant increase in throughput over the pure PHP protobuf implementation. See https://cloud.google.com/php/grpc for installation instructions."
    }
}
