{
    "name": "google/access-context-manager",
    "description": "Access Context Manager Client for PHP",
    "license": "Apache-2.0",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Google\\Identity\\AccessContextManager\\": "src",
            "GPBMetadata\\Google\\Identity\\Accesscontextmanager\\": "metadata"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Google\\Identity\\AccessContextManager\\Tests\\": "tests"
        }
    },
    "extra": {
        "component": {
            "id": "access-context-manager",
            "path": "AccessContextManager",
            "entry": null,
            "target": "googleapis/php-access-context-manager.git"
        }
    },
    "require": {
        "google/gax": "^1.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0|^8.0",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "conflict": {
        "google/cloud-asset": "<=1.4.2"
    },
    "suggest": {
        "ext-grpc": "Enables use of gRPC, a universal high-performance RPC framework created by Google.",
        "ext-protobuf": "Provides a significant increase in throughput over the pure PHP protobuf implementation. See https://cloud.google.com/php/grpc for installation instructions."
    }
}
